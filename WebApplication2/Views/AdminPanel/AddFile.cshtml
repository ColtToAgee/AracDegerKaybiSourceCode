@model EntityLayer.Concrete.MyFile
@{
    ViewBag.Title = "AddFile";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h2>Dosya Ekle</h2>
<br />
@using (Html.BeginForm("AddFile", "AdminPanel", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
<div class="form-group">
    @Html.Label("Dosyanın Müşterisi")
    @Html.DropDownListFor(x => x.UserId, (List<SelectListItem>)ViewBag.vu, new { @class = "form-control" })
    <br />
    @Html.TextBox("file", "", new { type = "file" }) <br />
    <br />
    <button class="btn btn-warning" type="submit">Kaydet</button>

</div>
}
